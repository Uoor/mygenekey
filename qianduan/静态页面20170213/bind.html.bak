		<script>
			window.onload=function()
			{
				var oT=document.getElementById('boxCode');
				oT.onkeydown=function(ev)
				{
					var oW=oT.value;
					var oEvent=ev||event;
					if(oEvent.keyCode==8)
					{
						if(oW)
						{
							for(var i=0;i<oW.length;i++)
							{
								var newStr=oW.replace(/\s$/g,'');
							}
							oT.value=newStr
						}
					}else{
						for(var i=0;i<oW.length;i++)
						{
							var arr=oW.split('');

							if((i+1)%5==0)
							{
								arr.splice(i,0,'-');
							}
						}
						oT.value=arr.join('');
					}
				}
			}
		</script>
